CREATE TABLE store (
    id INTEGER PRIMARY KEY,
    item TEXT,
    brand TEXT,
    price INTEGER,
    origin INTEGER,
    color INTEGER
);
INSERT INTO store VALUES (1,'bra', 'Calvin Klein', 42, 'Vietnam', 'Red');

